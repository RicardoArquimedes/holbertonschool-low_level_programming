#include "holberton.h"
/**
 *_strncat - copy string to another string.
 *@dest: char
 *@src: char
 *@n: char
 *Return: dest
 *
 */

char *_strncat(char *dest, char *src, int n)
{
	int i;
	int j;

	for (i = 0; dest[i] != '\0'; i++)
	{
	}
	for (j = 0; j < n && src[j] != '\0'; j++)
	{
		dest[i + j] = src[j];
	}
	return (dest);
}
